#include <bits/stdc++.h>
using namespace std;
#define fast ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define ll long long
#define endl '\n'
 
int main() {
    fast;
    int lol;
    cin >>lol;
    while (lol--) {
        int n, m;
        cin >> n >> m;
        string a;
        cin >> a;
        vector<int> freq(7, 0);
        for (char c : a) freq[c - 'A']++;
        int need = 0;
        for (int i = 0; i < 7; i++) {
            if (freq[i] < m) need += m - freq[i];
        }
        cout << need << endl;
    }
    return 0;
}
